     {
       "type": 12,
       "content": {
         "version": "NotebookGroup/1.0",
         "groupType": "editable",
         "title": "Recent Submissions",
         "items": [
           {
             "type": 9,
             "content": {
               "version": "KqlParameterItem/1.0",
               "parameters": [
                 {
                   "id": "ff837db0-dbec-472a-bb1d-2068a237e072",
